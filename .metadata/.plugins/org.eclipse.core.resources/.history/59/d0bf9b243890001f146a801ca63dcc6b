package kr.ac.kku.cs.wp.nangkongmu.user.entity;

import java.util.Date;
import java.util.List;
import java.util.Objects;
import java.io.Serializable; // Serializable 인터페이스를 사용하기 위해 추가


import jakarta.persistence.*;

@Embeddable
public class UserRoleId implements Serializable {

    @Column(name = "user_id", length = 200)
    private String userId;

    @Column(name = "role_id", length = 200)
    private String roleId;

    // Constructors - default, fields
    public UserRoleId() {}

    public UserRoleId(String userId, String roleId) {
        this.userId = userId;
        this.roleId = roleId;
    }

}
